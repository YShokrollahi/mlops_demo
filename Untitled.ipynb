   "source": [
    "# prepare_data.py\n",
    "from sklearn.datasets import load_iris\n",
    "import pandas as pd\n",
    "\n",
    "# Load Iris dataset\n",
    "data = load_iris()\n",
    "df = pd.DataFrame(data.data, columns=data.feature_names)\n",
    "df['target'] = data.target\n",
    "\n",
    "# Save dataset to CSV\n",
    "df.to_csv('iris.csv', index=False)\n"
   ]
  }
